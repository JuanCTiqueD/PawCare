        app:layout_constraintVertical_bias="0.206" />

    <TextView
        android:id="@+id/editarperfil3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Editar perfil"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.29"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.239"
        tools:ignore="MissingConstraints" />

    <View
        android:id="@+id/separator20"
        android:layout_width="379dp"
        android:layout_height="1dp"
        android:background="#000"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.456" />

    <ImageView
        android:id="@+id/imageView61"
        android:layout_width="61dp"
        android:layout_height="49dp"
        app:layout_constraintBottom_toBottomOf="@+id/btnEliminar4"
        app:layout_constraintEnd_toEndOf="@+id/btnEliminar4"
        app:layout_constraintHorizontal_bias="0.111"
        app:layout_constraintStart_toStartOf="@+id/btnEliminar4"
        app:layout_constraintTop_toTopOf="@+id/btnEliminar4"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/eliminar" />

    <Button
        android:id="@+id/btnProbarChatConfig"
        android:layout_width="103dp"
        android:layout_height="52dp"
        android:layout_marginTop="24dp"
        android:text="Chat "
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.173"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnEliminar"
        tools:ignore="UnknownIdInLayout" />

</androidx.constraintlayout.widget.ConstraintLayout>