<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    tools:context=".Hacer_solicitud">

    <ImageView
        android:id="@+id/imageView64"
        android:layout_width="45dp"
        android:layout_height="42dp"
        app:srcCompat="@drawable/eliminar"
        tools:layout_editor_absoluteX="137dp"
        tools:layout_editor_absoluteY="629dp" />

    <Button
        android:id="@+id/btnEliminar3"
        android:layout_width="167dp"
        android:layout_height="47dp"
        android:layout_marginBottom="56dp"
        android:backgroundTint="#C64928"
        android:clickable="true"
        android:text="         Eliminar "
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/etCondicion"
        app:layout_constraintVertical_bias="0.682" />

    <ImageView
        android:id="@+id/imageView63"
        android:layout_width="45dp"
        android:layout_height="42dp"
        app:layout_constraintBottom_toBottomOf="@+id/btnEliminar2"
        app:layout_constraintEnd_toEndOf="@+id/btnEliminar2"
        app:layout_constraintHorizontal_bias="0.171"
        app:layout_constraintStart_toStartOf="@+id/btnEliminar2"
        app:layout_constraintTop_toTopOf="@+id/btnEliminar2"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/eliminar" />

    <ImageView
        android:id="@+id/imageView52"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/editdueno"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="-3dp" />

    <TextView
        android:id="@+id/textView25"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:text="Editar solicitud"
        android:textColor="#242760"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/imageView52"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="@+id/imageView52"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.422"
        tools:gravity="center" />

    <ImageView
        android:id="@+id/imageView56"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.064"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.072"
        app:srcCompat="?attr/homeAsUpIndicator" />


    <EditText
        android:id="@+id/etFechaInicio"
        android:layout_width="0dp"
        android:layout_height="48dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="176dp"
        android:layout_marginEnd="20dp"
        android:background="@drawable/rounded_text"
        android:clickable="true"
        android:focusable="false"
        android:hint="Fecha Inicio"
        android:padding="10dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/etFechaFinal"
        android:layout_width="0dp"
        android:layout_height="48dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="20dp"
        android:background="@drawable/rounded_text"
        android:clickable="true"
        android:focusable="false"
        android:hint="Fecha Final"
        android:padding="10dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/etFechaInicio" />

    <EditText
        android:id="@+id/etHoraInicio"
        android:layout_width="0dp"
        android:layout_height="48dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="20dp"
        android:background="@drawable/rounded_text"
        android:clickable="true"
        android:focusable="false"
        android:hint="Hora Inicio"
        android:padding="10dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/etFechaFinal" />

    <EditText
        android:id="@+id/etHoraFinal"
        android:layout_width="0dp"
        android:layout_height="48dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="20dp"
        android:background="@drawable/rounded_text"
        android:clickable="true"
        android:focusable="false"
        android:hint="Hora Final"
        android:padding="10dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/etHoraInicio" />

    <EditText
        android:id="@+id/etUbicacion"
        android:layout_width="0dp"
        android:layout_height="48dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="20dp"
        android:background="@drawable/rounded_text"
        android:hint="UbicaciÃ³n"
        android:padding="10dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/etHoraFinal" />

    <EditText
        android:id="@+id/etServicio"
        android:layout_width="0dp"
        android:layout_height="48dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="20dp"
        android:background="@drawable/rounded_text"
        android:hint="Servicio"
        android:padding="10dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/etUbicacion" />

    <Button
        android:id="@+id/btnGuardar"
        android:layout_width="301dp"
        android:layout_height="48dp"
        android:backgroundTint="#80AA3FEC"
        android:clickable="true"
        android:text="COMFIRMAR"
        app:layout_constraintBottom_toTopOf="@+id/btnEliminar3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/etServicio" />

    <ImageView
        android:id="@+id/imageView68"
        android:layout_width="45dp"
        android:layout_height="42dp"
        app:layout_constraintBottom_toBottomOf="@+id/btnEliminar3"
        app:layout_constraintEnd_toEndOf="@+id/btnEliminar3"
        app:layout_constraintHorizontal_bias="0.122"
        app:layout_constraintStart_toStartOf="@+id/btnEliminar3"
        app:layout_constraintTop_toTopOf="@+id/btnEliminar3"
        app:srcCompat="@drawable/eliminar" />

</androidx.constraintlayout.widget.ConstraintLayout>
